<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Pet Vaccine Tree View--> 
    <record id="open_pet_vaccine_tree_view" model="ir.ui.view">
        <field name="name">pet.vaccine.tree</field>
        <field name="model">pet.vaccine</field>
        <field name="arch" type="xml">
            <tree string="Pet Vaccine">
                <field name='name'/>
            </tree>
        </field>
    </record>

    <!--Pet Vaccine Form View--> 
    <record id="open_pet_vaccine_form_view" model="ir.ui.view">
        <field name="name">pet.vaccine.form</field>
        <field name="model">pet.vaccine</field>
        <field name="arch" type="xml">
            <form string="Pet Vaccine">
                <sheet>  
                    <group>
                        <field name='name' required='True'/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Action for all view--> 
    <record id="action_pet_vaccine" model="ir.actions.act_window">
        <field name="name">Pet Vaccine</field>
        <field name="res_model">pet.vaccine</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="open_pet_vaccine_tree_view" />
    </record>

    <menuitem id="menu_pet_vaccine"
    name="Pet Vaccine"
    parent="menu_configuration"
    action='action_pet_vaccine'
    sequence="94"/>
</odoo>
