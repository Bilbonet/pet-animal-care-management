#. module: pet_animal_care_management
#: model:mail.template,body_html:pet_animal_care_management.vet_appointment_email_template
msgid "\n"
"<div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"    <p>Hello ${object.partner_id.name or ''},</p>\n"
"    <p>We inform you about the following Veterinary Appointment: </p>\n"
"\n"
"    <p style=\"border-left: 1px solid #8e0000; margin-left: 30px;\">\n"
"       &nbsp;&nbsp;Code: <strong>${object.name}</strong><br />\n"
"       &nbsp;&nbsp;Date: <strong>${format_tz(object.date_appointment, tz=object.partner_id.tz, format='%d/%m/%Y %H:%M:%S') or ''}</strong><br>\n"
"       &nbsp;&nbsp;Your Veterinarian: <a href=\"mailto:${object.veterinarian_id.work_email or ''}?subject=Vet Appointment%20${object.name}\"><strong>${object.veterinarian_id.name}</strong></a>\n"
"    </p>\n"
"    <br/>\n"
"    <p>If you have any question, do not hesitate to contact us.</p>\n"
"    <p>Thank you for trusting in  ${object.company_id.name or 'us'}!</p>\n"
"    % if object.veterinarian_id.user_id.signature:\n"
"       ${object.veterinarian_id.user_id.signature | safe}\n"
"    % else\n"
"        <div style=\"padding:0px;margin:auto;\">\n"
"            <table cellspacing=\"0\" cellpadding=\"0\">\n"
"                <tbody>\n"
"                    <tr>\n"
"                        <td style=\"padding:10px 10px 10px 0px;font-size: 12px\">\n"
"                            <img src=\"/logo.png\" style=\"padding: 0px; margin: 0px; height: auto; width: 100px;\" alt=\"${object.company_id.name}\"/>\n"
"                        </td>\n"
"                    </tr>\n"
"                </tbody>\n"
"            </table>\n"
"        </div>\n"
"    % endif\n"
"    <br/>\n"
"</div>\n"
"        "
msgstr ""

